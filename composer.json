{
  "name": "iods/module-optimize",
  "description": "A SEO library for getting the most out of Magento Catalog data.",
  "type": "magento2-module",
  "version": "000.1.1",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Rye Miller",
      "email": "rye@drkstr.dev",
      "homepage": "https://ryemiller.io",
    }
  ],
  "minimum-stability": "dev",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "require": {
    "php": ">=8.1",
    "iods/module-base": "*",
    "iods/module-baseui": "*",
    "iods/module-connect": "*",
    "iods/module-sysconfig": "*",
    "iods/module-bunyan": "*",
    "iods/module-cream": "*",
    "iods/module-frostbite": "*",
    "magento/framework": ">=103.0.5-p1",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-phpunit": "*",
    "phpunit/phpunit": "*",
    "composer/composer": "*@dev"
  },
  "autoload": {
    "files": ["registration.php"],
    "psr-4": {
      "Iods\\Optimize\\": "src"
    }
  },
  "scripts": {
    "test": [
      "@phpstan",
      "@phpunit"
    ],
    "phpstan": "phpstan analyse",
    "phpunit": "phpunit --no-coverage"
  }
}