<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <attribute name="itemtype" value="" />
        <attribute name="itemscope" value="" />
        <referenceBlock class="Web200\Seo\Block\Catalog\Product\Breadcrumbs" name="breadcrumbs" template="Magento_Theme::html/breadcrumbs.phtml"/>

        <referenceBlock name="before.body.end">
            <block class="Web200\Seo\Block\MicroData\Product" name="micro_data_product" ifconfig="seo/microdata/active" template="Web200_Seo::microdata/default.phtml" />
        </referenceBlock>
    </body>
</page>